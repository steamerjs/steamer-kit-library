  },
  "homepage": "https://github.com/steamerjs/steamer-kit-library#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "chai": "^4.1.2",
    "codecov.io": "^0.1.6",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-alloy": "^1.4.2",
